<h2>Edit Here</h2>
  <%= turbo_frame_tag @acc do %>

    <%= form_with(model: @acc, url: "/acc/1", local: true, data: { turbo_frame: @acc}) do |form| %>

        <div>
          <%= form.label :email , style: "display: block" %>
          <%= form.text_field :email %>
        </div>

        <div>
          <%= form.label :phone, "Phone Number", style: "display: block" %>
          <%= form.text_field :phone %>
        </div>
      <div class="mt-3">
        <%= form.submit class: "btn btn-outline-primary" %>
      </div>
      <div class= "mt-3">
        <%= link_to "Cancel Update", admin_path, class: "btn btn-outline-danger" %>
      </div>
  <% end %>
<% end %>
